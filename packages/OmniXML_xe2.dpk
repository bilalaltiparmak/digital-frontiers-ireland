package OmniXML_xe2;

{$R *.res}
{$IFDEF IMPLICITBUILDING This IFDEF should not be used by users}
{$ALIGN 8}
{$ASSERTIONS ON}
{$BOOLEVAL OFF}
{$DEBUGINFO ON}
{$EXTENDEDSYNTAX ON}
{$IMPORTEDDATA ON}
{$IOCHECKS ON}
{$LOCALSYMBOLS ON}
{$LONGSTRINGS ON}
{$OPENSTRINGS ON}
{$OPTIMIZATION OFF}
{$OVERFLOWCHECKS OFF}
{$RANGECHECKS ON}
{$REFERENCEINFO ON}
{$SAFEDIVIDE OFF}
{$STACKFRAMES ON}
{$TYPEDADDRESS OFF}
{$VARSTRINGCHECKS ON}
{$WRITEABLECONST ON}
{$MINENUMSIZE 1}
{$IMAGEBASE $400000}
{$DEFINE DEBUG}
{$ENDIF IMPLICITBUILDING}
{$DESCRIPTION 'OmniXML'}
{$RUNONLY}
{$IMPLICITBUILD ON}

requires
  vcl;

contains
  OmniXML in '3rdparty\omnixml\OmniXML.pas',
  GpTextStream in '3rdparty\omnixml\GpTextStream.pas',
  OmniXML_LookupTables in '3rdparty\omnixml\OmniXML_LookupTables.pas',
  GpStreamWrapper in '3rdparty\omnixml\GpStreamWrapper.pas',
  OmniXMLPersistent in '3rdparty\omnixml\OmniXMLPersistent.pas',
  OmniXMLProperties in '3rdparty\omnixml\OmniXMLProperties.pas',
  OmniXMLUtils in '3rdparty\omnixml\OmniXMLUtils.pas';

end.

